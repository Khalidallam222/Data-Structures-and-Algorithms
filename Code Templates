////////////////////////////////////////////////////////////////////////////////////////////////
# include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    scanf("%d", &n);
    while (n--) {
        char prob[100];
        scanf("%s", prob);
        if (prob[0] == 'P')
            printf("skipped\n");
        else {
            int pos1 = strchr(prob, '+') - prob;
            int a = atoi(&prob[0]);
            int b = atoi(&prob[pos1 + 1]);
            printf("%d\n", a + b);
        }
    }
    return 0;
}

//////////////////////////////////////////////////////////////////////////////////////
# include <bits/stdc++.h>
using namespace std;
int main(){
    int a, b;
    int i=1;
    // stop when both integers are 0
    while (scanf("%d %d", &a, &b), (a || b))
        printf("Case %d: &d", i++, );
    return 0;
}
////////////////////////////////////////////////////////////////////////////////////////////////
# include <bits/stdc++.h>
using namespace std;
int main(){
    char sentence [1000];
    int len;
    fgets(sentence, sizeof(sentence), stdin);
    len = strlen(sentence);
    printf("%c%d%c", sentence[0], len, sentence[len - 2]);
    return 0;
}

